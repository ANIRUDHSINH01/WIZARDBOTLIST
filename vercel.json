{
  "version": 2,
  "builds": [
    {
      "src": "api/submit.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/userinfo.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/submit",
      "dest": "api/submit.js"
    },
    {
      "src": "/api/userinfo",
      "dest": "api/userinfo.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
